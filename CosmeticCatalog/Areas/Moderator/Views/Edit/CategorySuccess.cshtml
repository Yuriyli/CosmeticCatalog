@model Category
@{
    ViewData["Title"] = "Категория изменена";
}
<div style="text-align:center;">
    <card title="Категория @("\"" + @Model.Name + "\"") успешно изменена">
        <form method="get" asp-area="Moderator" asp-controller="List" asp-action="Category">
            <button type="submit" class="btn btn-primary m-1">Перейти к спику категорий</button>
        </form>
    </card>
</div>
